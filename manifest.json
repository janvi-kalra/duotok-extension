{
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": {
    "default_title": "duotok inspiration",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": [
        "jquery.min.js",
        "prepare.js",
        "angular.min.js",
        "ng-file-model.min.js",
        "utils.js",
        "controller.js",
        "content_script.js"
      ],
      "matches": ["<all_urls>"],
      "runAt": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["tabs", "storage", "webRequest"],
  "host_permissions": ["https://www.netflix.com/*", "*://*.nflxvideo.net/*"],
  "description": "duotok inspiration",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 3,
  "name": "duotok inspiration",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.40"
}
